<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cg.dao.StockDao">
<!--    Integer AddStock(String articleId, Stock stock);-->
    <insert id="AddStock">
        insert into stock
            (article_id,in_time,out_time,salary,num,shop)
        values (#{articleId},#{inTime},#{outTime},#{salary},#{num},#{shop})
    </insert>


</mapper>
